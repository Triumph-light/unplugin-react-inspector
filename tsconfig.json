{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "jsx": "react",
        "lib": [
            "dom",
            "ESNext"
        ],
        "types": [
            "react",
            "react-dom"
        ],
        "module": "esnext",
        "target": "esnext",
        "allowSyntheticDefaultImports": true,
        "allowImportingTsExtensions": true,
        "noEmit": true
    }
}